<h1>Listing setores</h1>

<table>
  <tr>
    <th>Nome</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @setores.each do |setor| %>
  <tr>
    <td><%= setor.nome %></td>
    <td><%= link_to 'Show', setor %></td>
    <td><%= link_to 'Edit', edit_setor_path(setor) %></td>
    <td><%= link_to 'Destroy', setor, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setor', new_setor_path %>
